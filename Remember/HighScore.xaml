<Window x:Class="Remember.HighScore"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Remember"
        mc:Ignorable="d"
        Title="High Score" Height="534" Closing="Window_Closing">
    <Grid HorizontalAlignment="Left" VerticalAlignment="Top" Height="502" Width="759">
        <ListBox x:Name="ListBoxTime" HorizontalAlignment="Left" Height="402" VerticalAlignment="Top" Width="150" Margin="10,90,0,0"/>
        <ListBox x:Name="ListBoxClicks" HorizontalAlignment="Left" Height="402" VerticalAlignment="Top" Width="150" Margin="165,90,0,0"/>
        <Label x:Name="LabelTime" Content="By Time:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,59,0,0"/>
        <Label x:Name="LabelClicks" Content="By Clicks: " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="165,59,0,0"/>
    </Grid>
</Window>
